{
  "name": "ikagaka.sakurascriptplayer.js",
  "version": "1.2.0",
  "description": "ikagaka SakuraScript player",
  "url": "https://github.com/ikagaka/SakuraScriptPlayer.js",
  "keywords": [
    "ikagaka",
    "unstable",
    "ukagaka"
  ],
  "scripts": {
    "build": "npm run build-src; npm run build-test",
    "build-src": "node node_modules/coffee-script/bin/coffee -cm --join lib/SakuraScriptPlayer.js -- src/header.coffee src/*.coffee src/footer.coffee",
    "build-test": "node node_modules/coffee-script/bin/coffee -c test/*.coffee",
    "watch": "npm run build; grunt",
    "init": "npm update; bower update; npm run build; npm run watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ikagaka/SakuraScriptPlayer.js"
  },
  "dependencies": {
    "coffee-script": "^1.9.2"
  },
  "devDependencies": {
    "coffee-script": "",
    "grunt-contrib-connect": "",
    "grunt-contrib-watch": "",
    "grunt-shell": ""
  },
  "bugs": {
    "url": "https://github.com/Ikagaka/cuttlebone/issues"
  },
  "main": "lib/SakuraScriptPlayer.js",
  "author": "Ikagaka",
  "license": "MIT",
